<!DOCTYPE html>
<html lang="en">
    <%- include("../include/head.ejs") %>

<body>
	
	<!-- begin #page-container -->
	<div id="page-container" class="fade page-sidebar-fixed page-header-fixed">
		<!-- begin #header -->
		<%- include("../include/header.ejs") %>
		<!-- end #header -->
		
		
		<%- include("../include/sidebar.ejs") %>
	
	<div id="content" class="content">
		
		<!-- begin row -->
		<div class="row">
			<div class="col-xl-12">
				<div class="panel panel-inverse">
					<!-- begin panel-heading -->
					<div class="panel-heading">
						<h4 class="panel-title">DataTable - Autofill </h4>
						<div class="panel-heading-btn">
							<a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>
							<a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>
							<a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
							<a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>
						</div>
					</div>
					<!-- end panel-heading -->
					<!-- begin alert -->
					<div class="alert alert-success fade show">
						<button type="button" class="close" data-dismiss="alert">
						<span aria-hidden="true">&times;</span>
						</button>
						<div class="progress rounded-corner" style="height: 30px;">
							<div class="progress-bar bg-primary" style="width: 25%">
							  Total Product:- 30
							</div>
							<div class="progress-bar bg-warning" style="width: 25%"
							  >Total Views:- 50
							</div>
							<div class="progress-bar bg-success" style="width: 25%"
							  >Stacked
							</div>
							<div class="progress-bar bg-success" style="width: 25%"
							  >Stacked
							</div>
							
						  </div>
					</div>
					
					<!-- end alert -->
					<!-- begin panel-body -->
					<div class="panel-body">
						<table id="data-table-autofill" class="table table-striped table-bordered table-td-valign-middle">
							<thead>
								<tr>
									<th width="1%">No</th>
									<th class="text-nowrap">Image</th>
									<th class="text-nowrap">Title</th>
									<th class="text-nowrap">Number</th>
									<th class="text-nowrap">Description </th>
									<th class="text-nowrap" width="1%">Action</th>
								</tr>
							</thead>
							<tbody>
								<% if(posts.length > 0 ) { %>
									<%posts.forEach((post)=>{%>
									<tr class="gradeA">
										<td>3</td>
										<td width="4%" class="with-img"><img width="100" height="10" src="../assets/img2/<%=post.image%>" class="img-rounded height-70" /></td>
										<td><a href=""><%=post.name.slice(0,30)%>...</a></td>
										<td><%=post.number%></td>
										<td><%=post.detail.slice(0,50)%>...</td>
										<td class="with-btn" nowrap>
											<a href="#" class="btn btn-sm btn-primary width-60 m-r-2">Edit</a>
											<a href="#" class="btn btn-sm btn-white width-60">Delete</a>
										</td>
									</tr>
									<%})%>
								<%}else{%>
									<p>No Post In This Database</p>
								<%}%>
							</tbody>
						</table>
					</div>
					<!-- end panel-body -->
				</div>
			</div>
		</div>
		<!-- end row -->
	</div>
	<!-- end #content -->

 <!-- end #content -->
        
		<!-- begin scroll to top btn -->
		<a href="javascript:;" class="btn btn-icon btn-circle btn-primary btn-scroll-to-top fade" data-click="scroll-top"><i class="fa fa-angle-up"></i></a>
		<!-- end scroll to top btn -->
	</div>
	<!-- end page container -->
    <%- include("../include/jsfile.ejs") %>
</body>
</html>